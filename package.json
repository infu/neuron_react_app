{
  "name": "neutron-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "neutron-scripts": "^1.1.0",
    "neutron-tools": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "homepage": "/app/hello/",
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build && npm run mogen && ./postbuild.sh",
    "mogen": "node ./node_modules/neutron-scripts/src/mogen.js",
    "mopack": " node ./node_modules/neutron-scripts/src/mopack.js",
    "validate": "node ./node_modules/neutron-scripts/src/validate.js",
    "package": "npm run validate && npm run build && npm run mopack && node ./node_modules/neutron-scripts/src/pack.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
